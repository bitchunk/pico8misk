for j,t in pairs(htbl[[
x=40;
y=80;
t=dog;
i{collar bone frisbee{1 2 3}}
]]
) do
for i,v in pairs(type(t)=='table' and t or {t}) do
 local s
	if type(v)=='table' then
	 s='{'
		for i,w in pairs(v) do
			s=s..' '..w
		end
		s=s..' }'
	end
	print(j..'['..i..': '..(s or v)..']')
	end
end


