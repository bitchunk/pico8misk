//isdebug=true
pi=1
schex=0x6000
bfhex=0x0800
a=400
aa=pi/a
titlex=112
titley=48
titletext='∧◆push ❎ button◆∧'
function _init()
//stack=scenes.new('root')
stack=mkscenes({'key','draw','main','snd'})
local t=stack.main.push('maptitle',1)
stack.main.push('noisedraw',0)
stack.draw.push('drawtitle',0).settrg(t)
stack.snd.push('sndstart',1).settrg(t)
stack.key.push('keyck',0)
cls()
scenes.reg({
maptitle=function(ord)
cls()
//map(96,48,0,0,16,16,0)
fset(1,1)
map(96,48,0,0,16,16,1)
memcpy(bfhex,schex,0x400)
end,
drawtitle=function(ord)
map(titlex,titley,0,0,16,16)
if flr(ord.cnt/8)%8<6 then
print(titletext,20,64,6)
end
end,
fadetitle=function(ord)
local c=ord.cnt
local p=ord.prm
local a=64/ord.dur
local xa=c*a*8
local ya=c*a
local of
local fd=ord.prm.fade

if ord.isfst() then
 if p.ofs~=nil then
  c=flr(p.ofs)
  if c<0 then c=1 end
  ord.cnt=c
 end
end

if fd=='out' then
xa=c*a*8
ya=c*a
else
xa=-c*a*8
ya=64-(c*a)
end
map(titlex,titley,(xa)%128,-ya,16,8)
map(titlex,titley,((xa%128))-128,-ya,16,8)

map(titlex,titley+8,-(xa%128),ya+64,16,8)
map(titlex,titley+8,-((xa%128)-128),ya+64,16,8)

end,
noisedraw=function(ord)
local p=ord.prm
local c=ord.cnt
local a=400
local xa=sin(aa*c)
local bline=32
local blbl=bline*bline
local h=64
local w=128
local y
local x
local x2
local s
local xabd=pi*(30/xa)

--a line by 64 byte
for i=0,127 do
y=sin(xabd*(c+i))*bline
x=flr(y*bline)%w
//x=(y*bline)%w
y=flr(i-((y*blbl)+c))%8
//y=i%8
x2=flr(x/2)
//x2=x
--4byte=8px to 16loop
s=(x%2==1 and 4 or 0)
//if s==4 then
for j=0,15 do
 poke4(schex+(i*0x40)+(j*4)
 ,rotl(peek4(bfhex+(y*0x40)+(((j*4)+x2)%64)),s)
 )
end
//end
--memcopy refresh x with 2px
//memcpy(schex+(i*0x40),bfhex+((y)*0x40)+x,0x40)
end
//rectfill(0,0,128,5,0)
//print(a,0,0,7)
//0x4+0x4
//pc=(c/10)%16

//p=flr(c/10)%16--upd rate
//p1=p--noshift

//p=flr((c/10)+6)%16--upd rate
//p2=shl(p,4)--4bit shift

--set 16bit data 
//memset(0x6000,p1+p2,1)

//memset(0x6001,pc2,1)
//memset(0x6000,shl(0x1,((c%8)+0x8)),1)

end,
sndstart=function(ord)
if ord.isfst() then
local snid=({0,2})[flr(rnd(2))+1]

 music(snid,2000)
end
end,
noisese=function(ord)
local snid=({0,2})[flr(rnd(2))+1]
if ord.prm.fade=='in' then
 if ord.isfst() then
  music(-1,2000)
  sfx(2,3)
 end
else
 if ord.isfst() then
  sfx(3,4)
 elseif ord.islst() then
  music(snid,2000)
  sfx(-1)
 end
end

end,
keyck=function(ord)
//open title
if btnp(❎) then
 local t=stack.draw.rem('fadetitle')
 local p
 stack.draw.remall()
// if t==nil or t.nm~='fadetitle' then
 if t==nil or t.nm~='fadetitle' then
  p={fade='out'}
 else
  p={fade='out',ofs=140 - (140*(t.cnt/40))}
 end
// if #stack.draw.ords>0 then cls() stop() end
 stack.draw.push('fadetitle',140,p)
 
// cls() stop(stack.draw.ords[1].nm)

 stack.key.rem()
 stack.key.push('keyck2',0)
 stack.snd.rem()
 stack.snd.push('noisese',1,{fade='in'})
end
end,
keyck2=function(ord)
//back title
if btnp(❎) then
 local t=stack.draw.rem('fadetitle')
 local p
 stack.draw.remall()
 if t==nil or t.nm~='fadetitle' then
  p={fade='in'}
 else
  p={fade='in',ofs=40-(40*(t.cnt/140))}
 end
 stack.draw.push('fadetitle',40,p)
 stack.draw.push('drawtitle',0)
 stack.key.rem()
 stack.key.push('keyck',0)
 stack.snd.rem()
 stack.snd.push('noisese',40,{fade='out'})
end
end,

})
end
function _update60()
stack.main.tra()
stack.key.tra()
stack.snd.tra()
end

function _draw()
stack.draw.tra()
if isdebug then
rectfill(0,122,128,128,0)
print(stat(1),0,123,11)
stack.key.print()
end
end


