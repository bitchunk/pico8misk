--scene manager

scorder={}
scorder.new=function(fn,d,p)
local o={nm=fn,cnt=0,dur=d,rem=false,fnena=nil,trg=nil}
o.prm=(p==nil and {} or p)
o.settrg=function(t)
o.trg=t
end
o.isfst=function()
return o.cnt==0
end
o.islst=function()
return o.cnt>=(o.dur-1)
end

return o
end


scenes={regfn={}}
scenes.reg=function(fns)
fns=type(fns)=='table' and fns or {fns}
for k,f in pairs(fns) do
 scenes.regfn[k]=f
end
end

scenes.new=function(key)
local o={nm=key,sccur=nil}
o.ords={}
o.curo=nil
o.push=function(fn,d,p)

if p~=nil and fn=='fadetitile' then cls() stop(#p) end
 local sc=scorder.new(fn,d,p)
 add(o.ords,sc)
 return sc
end
o.rem=function(key)
 local s
 if key==nil then s=o.cur()
 else s=o.find(key)
 end
 if s==nil then return nil end
 s.rem=true
 del(o.ords,s)
// if s.nm=='fadetitle' and #o.ords>0 then cls() stop(o.ords[1].nm) end
 return s
end
o.remall=function()
 local s={}
 for v in all(o.ords) do
 add(s,o.rem(v.nm))
 end
// cls() stop(#o.ords)
 return s
end
o.find=function(key)
 for v in all(o.ords) do
  if v.nm==key then return v end

 end
 return nil
end
o.cur=function()
return o.ords[1]
end
o.shift=function()
local v=o.ords[1]
del(o.ords[1])
return v
end
o.tra=function()
local c=o.cur()
 c=c==nil and o.shift() or c
 o.sccur=c
 
	if c==nil and #o.ords==0 then
		return
	end
	if c.trg~=nil then
	 if c.trg.rem then c.trg=nil end
// 	c.trg=(c.trg.rem and nil or c.trg)
//	print(c.nm..tostr(c.trg.rem))
 	return
 end
 
	if c.fnena==nil then
		c.fnena=(c.nm==nil or scenes.regfn[c.nm]~=nil)
		if c.fnena==false then
			print('notfound scene: '..c.nm)
		end
	end
	
	if c.fnena then
		if c.rem or (c.nm~=nil and scenes.regfn[c.nm](c)) then
			o.rem()
		end
	end
 
 if(c.remtrg~=nil and c.rmtri.rem) then
  o.rem()
 end
 c.cnt+=1
 if(c.dur>0 and c.dur <= c.cnt) then
  o.rem()
 end
end
o.print=function()
 for i,v in pairs(o.ords) do
 color((i%12)+1)
 print(v.nm..':'..v.cnt)
 end
end
return o
end

function mkscenes(keys)
local o={}
if type(keys)=='string' then keys={keys} end
for v in all(keys) do
 o[v]=scenes.new(v)
end
return o
end
